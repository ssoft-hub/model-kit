# global
CONFIG_PATH = $${PWD}/../../../..
for( tmp, 1..20 ) {
    CONFIG_FILE = $${CONFIG_PATH}/project/qmake/config.prf
    exists( $${CONFIG_FILE} ) {
        include( $${CONFIG_FILE} )
        break()
    }
    unset(CONFIG_FILE)
    CONFIG_PATH = $${CONFIG_PATH}/..
}

!exists( $${CONFIG_FILE} ) {
    error( The complex configuration file not found (see README information in $${PWD}/readme*.txt) )
}
